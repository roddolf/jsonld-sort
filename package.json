{
  "name": "jsonld-sorter",
  "version": "0.2.0",
  "description": "Sorter of JSON-LD data",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "bin": "dist/cli.js",
  "scripts": {
    "start": "npm run build",
    "build": "npm-run-all build:*",
    "build:src": "start buildDist && start buildCLI",
    "build:docs": "start buildDocs",
    "test": "if [[ ${CI} ]]; then npm run test:ci; else npm run test:dev; fi",
    "test:dev": "start test",
    "test:ci": "start testCI"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roddolf/jsonld-sort.git"
  },
  "keywords": [
    "jsonld",
    "sorter"
  ],
  "author": "Rodolfo Aguirre <aguirreg.rodolfo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roddolf/jsonld-sort/issues"
  },
  "homepage": "https://github.com/roddolf/jsonld-sort#readme",
  "devDependencies": {
    "@start/cli": "^0.5.0",
    "@start/plugin": "^0.3.1",
    "@start/plugin-find": "^0.3.1",
    "@start/plugin-lib-jest": "^0.5.0",
    "@start/plugin-lib-rollup": "^0.3.0",
    "@start/plugin-read": "^0.3.1",
    "@start/plugin-remove": "^0.3.1",
    "@start/plugin-sequence": "^0.4.0",
    "@start/plugin-write": "^0.3.2",
    "@start/reporter-verbose": "^0.2.1",
    "@types/jest": "^24.0.19",
    "@types/yargs": "^13.0.3",
    "coveralls": "^3.0.7",
    "jest": "^24.9.0",
    "microbundle": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node-register": "^1.0.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  },
  "start": {
    "file": "./tasks",
    "reporter": "@start/reporter-verbose",
    "require": [
      [
        "ts-node-register",
        {
          "project": "./tasks/tsconfig.json",
          "transpileOnly": true
        }
      ]
    ]
  },
  "dependencies": {
    "yargs": "^13.2.0"
  }
}
